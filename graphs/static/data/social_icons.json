{
    "facebook": {
        "href": "https://www.facebook.com/sharer/sharer.php?u={url}",
        "src": "data:image/svg+xml;base64,"
    },
    "telegram": {
        "href": "https://t.me/share/url?text={body}&url={url}",
        "src": "data:image/svg+xml;base64,"
    },
    "whatsapp": {
        "href": "https://api.whatsapp.com/send?text={body}",
        "src": "data:image/svg+xml;base64,"
    },
    "reddit": {
        "href": "https://www.reddit.com/submit?title={title}&url={url}",
        "src": "data:image/svg+xml;base64,"
    },
    "mail": {
        "href": "mailto:?subject={title}&body={body}",
        "src": "data:image/svg+xml;base64,"
    }
}